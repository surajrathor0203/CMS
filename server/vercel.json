{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "index.js",
            "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"],
            "headers": {
                "Access-Control-Allow-Origin": "*",
                "Access-Control-Allow-Headers": "Origin, X-Requested-With, Content-Type, Accept"
            }
        }
    ],
    "env": {
        "NODE_ENV": "production"
    },
    "regions": ["bom1"],
    "functions": {
        "api/*.js": {
            "memory": 1024,
            "maxDuration": 10
        }
    }
}